{
  "name": "next-playground",
  "author": "@romMidnight",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "lint": "eslint . --ext jsx --ext js"
