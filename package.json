      "yarn format"
    ]
  },
  "dependencies": {
    "moment": "^2.27.0",
    "next": "^9.4.4",
    "next-auth": "^3.3.0",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-big-calendar": "^0.26.0",
    "react-dom": "^16.13.1",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.2",
    "tailwindcss": "^1.4.6"
  }
